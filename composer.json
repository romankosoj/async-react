{
	"name" : "koolkode/async-react",
	"description" : "KoolKode Async React Integration",
	"keywords" : [
		"koolkode",
		"async",
		"react"
	],
	"type" : "library",
	"license" : "MIT",
	"homepage" : "https://github.com/koolkode/async-react",
	"authors" : [{
			"name" : "Martin Schr√∂der",
			"email" : "m.schroeder2007@gmail.com"
		}
	],
	"minimum-stability" : "dev",
	"prefer-stable" : true,
	"require" : {
		"php" : "~7.0",
		"koolkode/async" : "@dev",
		"react/event-loop" : "~0.4"
	},
	"require-dev" : {
		"phpunit/phpunit" : "~5.2",
		"react/stream" : "~0.4",
		"react/promise" : "~2.0"
	},
	"autoload" : {
		"psr-4" : {
			"KoolKode\\Async\\React\\" : "src/"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"KoolKode\\Async\\React\\" : "test/src/"
		},
		"files" : [
		    "src/functions.php"
		]
	},
	"extra" : {
		"branch-alias" : {
			"dev-master" : "0.1.x-dev"
		}
	}
}